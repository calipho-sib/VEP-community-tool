  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "feature-viewer": "1.4.2",
    "http-proxy-middleware": "^2.0.6",
    "match-sorter": "^6.3.0",
    "papaparse": "^5.3.1",
    "react": "^17.0.2",
    "react-csv-reader": "^3.3.1",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.1",
    "react-table": "^7.7.0",
    "react-table-plugins": "^1.3.1",
